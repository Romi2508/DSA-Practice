#include <iostream>
using namespace std;
int main()
{
    int a[5];
    int n = 5;
    cout << "enter the elements" << endl;
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    for (int j = 0; j < n; j++)
    {
        if (a[j] <= a[j + 1])
        {

        }
        else{
            cout << "Not Sorted";
        }
    }
    cout << "Sorted";
