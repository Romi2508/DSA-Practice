#include <iostream>
#include <vector>
using namespace std;

int singleNumber(vector<int>& nums) {
    int res = 0;
    for (int x : nums) {
        res ^= x;
    }
    return res;
}

int main() {
    vector<int> nums = {4, 4, 2, 1, 1};

    cout << "Single number is: " << singleNumber(nums) << endl;

    return 0;
}
