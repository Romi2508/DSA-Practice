#include <iostream>
#include <vector>
using namespace std;

int singleNumber(vector<int>& nums) {
    int res = 0;
    for (int i = 0; i < nums.size(); i++) {
    int x = nums[i]; 
        res ^= x;
    
    return res;
}

int main() {
    vector<int> nums = {4, 4, 2, 1, 1};

    cout << "Single number is: " << singleNumber(nums) << endl;

    return 0;
}
